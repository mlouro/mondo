{
  "name": "mondohood",
  "version": "1.0.3",
  "type": "app",
  "dependencies": {
    "hoodie-server": "0.9.2",
    "hoodie-plugin-appconfig": "~0.1.0",
    "hoodie-plugin-email": "~0.1.1",
    "hoodie-plugin-users": "0.0.9"
  },
  "subdomain": "hoodie-mondohood",
  "domains": [
    "admin.hoodie-mondohood.jit.su",
    "couch.hoodie-mondohood.jit.su"
  ],
  "scripts": {
    "start": "node node_modules/hoodie-server/bin/start"
  },
  "hoodie": {
    "plugins": [
      "hoodie-plugin-appconfig",
      "hoodie-plugin-email",
      "hoodie-plugin-users"
    ]
  },
  "browser": {
    "jquery": "./src/vendor/jquery/jquery-2.0.3.js"
  },
  "browserify": {
    "transform": [
      "reactify"
    ]
  },
  "dependencies": {
    "browserify": "*",
    "underscore": "~1.5.2",
    "react": "~0.8.0"
  }
}